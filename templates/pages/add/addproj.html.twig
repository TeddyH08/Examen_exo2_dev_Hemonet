{% extends 'base.html.twig' %}

{% block body %}
    <div class="connexion_container">
        <h2 class="title_container">Ajout d'un projet</h2>

        {% for message in app.flashes('error') %}
            <div class="error">
                {{ message }}
            </div>
        {% endfor %}

        <div class="connexion_form">
            {{ form(form) }}
        </div>
    </div>

    <div class="btn_return">
        <a href="{{ path('admin') }}" class="return">Retour</a>
    </div>
{% endblock %}